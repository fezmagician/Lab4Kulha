<Window x:Class="A1_Graphs.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:A1_Graphs"
		mc:Ignorable="d"
		Title="Graphs" Height="500" Width="700" Loaded="Window_Loaded" SizeChanged="Window_SizeChanged">
	<Grid>
		<Canvas x:Name="GraphCanvas">
			<Polyline x:Name="Graph1"
				Stroke="Red"
				StrokeThickness="1"
				Canvas.Top="242"
				Canvas.Left="350"
			/>
			<Polyline x:Name="Graph2"
				Stroke="Green"
				StrokeThickness="1"
				Canvas.Top="242"
				Canvas.Left="350"
			/>
			<Polyline x:Name="Graph3"
				Stroke="Blue"
				StrokeThickness="1"
				Canvas.Top="242"
				Canvas.Left="350"
			/>
            <Polyline x:Name="Graph3_L"
				Stroke="Blue"
				StrokeThickness="1"
				Canvas.Top="242"
				Canvas.Left="350"
			/>
        </Canvas>

	</Grid>
</Window>
